{
  "name": "v",
  "version": "0.0.0",
  "engines": {
    "node": "8.11.2"
  },
  "private": true,
  "scripts": {
    "dev:front": "cd client && npm run dev",
    "dev:server": "ts-node-dev --respawn --transpileOnly ./api/app.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:front\"",
    "tsc": "tsc",
    "clean": "rimraf ./build",
    "build": "tsc && node ./build/app.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "heroku-prebuild": "concurrently \"npm install --dev\" \"cd client npm install --dev\""
  },
  "dependencies": {
    "@types/express": "^4.17.1",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-validator": "^6.2.0",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "mongoose": "^5.7.7",
    "morgan": "~1.9.1",
    "rimraf": "^2.6.3",
    "ts-node-dev": "^1.0.0-pre.43"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.20.0",
    "typescript": "^3.6.4"
  }
}
